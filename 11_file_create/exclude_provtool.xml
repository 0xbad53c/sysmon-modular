<Sysmon schemaversion="4.22">
   <EventFiltering>
 <RuleGroup name="" groupRelation="or">
      <FileCreate onmatch="exclude">
			<!--SECTION: Microsoft:Windows-->
			<SourceImage condition="is">c:\windows\system32\provtool.exe</SourceImage> <!-- Microsoft:Windows:Provisioning package runtime processing tool -->
		</FileCreate>
</RuleGroup>
</EventFiltering>
</Sysmon>
